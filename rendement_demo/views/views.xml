<odoo>
  <data>
    <!-- explicit list view definition -->

      <record model="ir.ui.view" id="rendement_subscription_tree">
          <field name="name">rendement_demo_list</field>
          <field name="model">rendement.subscription</field>
          <field name="arch" type="xml">
              <tree>
                  <field name="subscription_number"/>
                  <field name="partner_id"/>
                  <field name="name"/>
                  <field name="start_date"/>
                  <field name="end_date"/>
                  <field name="price"/>
              </tree>
          </field>
      </record>

      <record model="ir.ui.view" id="rendement_subscription_form">
            <field name="name">rendement_demo_form</field>
            <field name="model">rendement.subscription</field>
            <field name="arch" type="xml">
                <form>
                    <group col="4">
                        <field name="subscription_number"/>
                        <field name="partner_id" context="{'res_partner_search_mode': 'customer', 'show_address': 1, 'show_vat': True}" options="{&quot;always_reload&quot;: True}"/>
                        <field name="name"/>
                        <field name="start_date"/>
                        <field name="end_date"/>
                        <field name="price"/>
                    </group>
                </form>
            </field>
        </record>

    <!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="action_window">
      <field name="name">Rendement Subscription</field>
      <field name="res_model">rendement.subscription</field>
      <field name="view_mode">tree,form</field>
    </record>

    <!-- server action to the one above -->
    <!--
    <record model="ir.actions.server" id="rendement_demo.action_server">
      <field name="name">rendement_demo server</field>
      <field name="model_id" ref="model_rendement_demo_rendement_demo"/>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": self._name,
        }
      </field>
    </record>
    -->

    <!-- Top menu item -->
      <menuitem id="menu_root"
            name="Rendement"
            web_icon="sale_management,static/description/icon.png"
            sequence="10"/>
     <!--menu categories -->
      <menuitem id="menu_1"
            name="Rendement Subscription"
            parent="menu_root"
            sequence="2"/>

      <menuitem id="menu_2"
            name="Rendement Subscription"
            parent="menu_1"
            action="action_window"
            sequence="1"/>

      <record model="ir.ui.view" id="rendement_mailing_tree">
          <field name="name">rendement_mailing_demo_list</field>
          <field name="model">rendement.mailing</field>
          <field name="arch" type="xml">
              <tree>
                  <field name="name"/>
                  <field name="date"/>
                  <field name="bounced"/>
              </tree>
          </field>
      </record>

      <record model="ir.ui.view" id="rendement_mailing_form">
            <field name="name">rendement_mailing_demo_form</field>
            <field name="model">rendement.mailing</field>
            <field name="arch" type="xml">
                <form>
                    <group col="4">
                        <field name="name"/>
                        <field name="date"/>
                        <field name="bounced"/>
                    </group>
                </form>
            </field>
        </record>

    <!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="action_mailing_window">
      <field name="name">Rendement Mailing</field>
      <field name="res_model">rendement.mailing</field>
      <field name="view_mode">tree,form</field>
    </record>

      <menuitem id="menu_3"
            name="Rendement Mailing"
            parent="menu_1"
            action="action_mailing_window"
            sequence="2"/>

      <record model="ir.ui.view" id="rendement_newsletter_tree">
          <field name="name">rendement_newsletter_demo_list</field>
          <field name="model">rendement.newsletter</field>
          <field name="arch" type="xml">
              <tree>
                  <field name="name"/>
                  <field name="date"/>
              </tree>
          </field>
      </record>

      <record model="ir.ui.view" id="rendement_newsletter_form">
            <field name="name">rendement_newsletter_demo_form</field>
            <field name="model">rendement.newsletter</field>
            <field name="arch" type="xml">
                <form>
                    <group col="4">
                        <field name="name"/>
                        <field name="date"/>
                    </group>
                </form>
            </field>
        </record>

    <!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="action_newsletter_window">
      <field name="name">Rendement Newsletter</field>
      <field name="res_model">rendement.newsletter</field>
      <field name="view_mode">tree,form</field>
    </record>

      <menuitem id="menu_4"
            name="Rendement newsletter"
            parent="menu_1"
            action="action_newsletter_window"
            sequence="3"/>



  </data>
</odoo>